{
    "optimization_method": "SpacePartitioning",
    "space_partitioning_settings": {
        "top_k": 4,
        "partitions_per_trial": 5,
        "use_clustering": true,
        "region_acquisition_strategy": "VIS",
        "boundary_box": {
            "op_0_to_1": [0, 4],
            "op_0_to_2": [0, 4],
            "op_0_to_3": [0, 4],
            "op_1_to_2": [0, 4],
            "op_1_to_3": [0, 4],
            "op_2_to_3": [0, 4]
        }
    },
    "n_trials": 20,
    "input_cost_per_1000_tokens": 0.000150,
    "output_cost_per_1000_tokens": 0.000600,
    "initial_samples": 5,
    "candidates_per_request": 5,
    "max_candidates_per_trial": 5,
    "evaluations_per_request": 5,
    "max_evaluations_per_trial": 5,
    "max_context_configs": 110,
    "max_requests_per_minute": 5000,
    "max_tokens_per_minute": 4000000,
    "benchmark": "NB201",
    "benchmark_settings": {
        "dataset": "cifar10",
        "device_metric": "titanx_256_latency"
    },
    "warmstarter": "RANDOM_WARMSTARTER",
    "candidate_sampler": "LLM_SAMPLER",
    "acquisition_function": "HypervolumeImprovement",
    "surrogate_model": "mooLLM_SUR_BATCH",
    "shuffle_icl_columns": false,
    "shuffle_icl_rows": true,
    "use_few_shot_examples": true,
    "context_limit_strategy": "LastN",

    "warmstarting_prompt_template": "./prompt_templates/NB201_space_partitioning/warmstarting.txt",
    "candidate_sampler_prompt_template": "./prompt_templates/NB201_space_partitioning/candidate_sampler.txt",
    "surrogate_model_prompt_template": "./prompt_templates/NB201_space_partitioning/surrogate_model.txt",

    "metrics": ["f1", "latency"], 
    "prompt": {
        "problem_description": "You are tasked with finding optimal neural network architectures for image classification on CIFAR-10.\n**Dataset**: CIFAR-10\n- 60,000 color images (32x32 pixels)\n- 10 classes with 6,000 images per class\n**Representation**: Each architecture is encoded as a sequence of integers\n- 0 = 'none'\n- 1 = 'skip_connect'\n- 2 = 'avg_pool_3x3'\n- 3 = 'nor_conv_1x1'\n- 4 = 'nor_conv_3x3'\n**Evaluation**: Architectures are measured by error and latency (lower is better)",
        "constraints": "",
        "metrics": "lower f1 and lower latency",
        "icl_examples_template": "Architecture configuration: $configuration \nF1: $f1, Latency: $latency\n",
        "icl_example_template":  "{\"op_0_to_1\": $op_0_to_1, \"op_0_to_2\": $op_0_to_2, \"op_0_to_3\": $op_0_to_3, \"op_1_to_2\": $op_1_to_2, \"op_1_to_3\": $op_1_to_3, \"op_2_to_3\": $op_2_to_3 }",
        "warmstarting_response_format": "{\"op_0_to_1\": ?, \"op_0_to_2\": ?, \"op_0_to_3\": ?, \"op_1_to_2\": ?, \"op_1_to_3\": ?, \"op_2_to_3\": ? }",
        "candidate_sampler_response_format": "{\"op_0_to_1\": ?, \"op_0_to_2\": ?, \"op_0_to_3\": ?, \"op_1_to_2\": ?, \"op_1_to_3\": ?, \"op_2_to_3\": ? }",
        "surrogate_model_response_format": "{\"F1\": ?, \"Latency\": ?}"
    }
}